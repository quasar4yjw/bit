(예제소스 2)
  static int v1;
  static int v2;
  static String op;
  static int result;
  
  static void (주1) {
    System.out.println("******************");
    System.out.printf("%d %s %d = %d\n",
        v1, op, v2, result);
    System.out.println("******************");
  }
  
  static void compute(){
    (주2)(op){
    case "+": result = v1 + v2; break;
    case "-": result = v1 - v2; break;
    case "*": result = v1 * v2; break;
    case "/": result = v1 / v2; break;
     }
  }
   
  public static void main(String[] args) {
    Scanner (주3) = new Scanner(System.in);
    System.out.print("값1? ");
    v1 = Integer.parseInt(scan.nextLine());

    System.out.print("값2? ");
    v2 = Integer.parseInt(scan.nextLine());

    System.out.print("연산자? ");
    op = scan.nextLine();

    compute();
    displayScore();
  }

