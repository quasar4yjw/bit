public class quiz {
  static int a = 10;
  static int b = 5;
  static int test() {
    int c = 20;
    return c;
  }
  public static void main(String[] args) {
    int d = test();
    plus(a, d);
    System.out.printf(b);
  }
  static void plus(int e, int x) {
    b =  e + x;
  }
}
