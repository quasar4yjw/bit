package java01;

public class Test24 {

  public static void main(String[] args) {
   
 1)   for (int i = 4; i < 100; i += 4) { 

 2)    System.out.printf("%d ", i);
 3)    if ((i % 20) != 0) {
 4)    System.out.println();
        if ((i % 60) == 0) {
 5)   System.out.print("@");
        }
      }
    }

  }

}

