    for (①int x = 0; ②x < 100; ③x += 5) {
      ④System.out.print(i + " ");
      if (x % 25 == 0) {
        System.out.println("\n---------------");
        if (x % 50 == 0) {
       ⑤   System.out.print("^");
        }
      }
    }


