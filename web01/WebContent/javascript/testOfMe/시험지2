package java01;

public class Test22 {

	public static void main(String[] args) {
		int x= 1;
		
		do{
			if(x%4==0){
				System.out.print(x + " ");
				if(x%20==0) {
				System.out.print("\n");
					if(x%60==0) {
					System.out.print("@");
					}
				}
			}
			x++;

		} while(x<=100);
		
	}

}
